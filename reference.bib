	file = {Cohan et al. - 2018 - SMHD A Large-Scale Resource for Exploring Online .pdf:/home/lunaire/Zotero/storage/JUJD9A5D/Cohan et al. - 2018 - SMHD A Large-Scale Resource for Exploring Online .pdf:application/pdf},
}

