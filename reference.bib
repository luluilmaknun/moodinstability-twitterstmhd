	pages = {63--73},
	file = {Gkotsis et al. - 2016 - The language of mental health problems in social m.pdf:/home/lunaire/Zotero/storage/AF7YQTXM/Gkotsis et al. - 2016 - The language of mental health problems in social m.pdf:application/pdf},
